/***************************{CSS Reset}*****************************/
body,html{ height:100%;}
html, body, div, span,
h1, h2, h3, h4, h5, h6, p, em, img, strong, i,
dl, dt, dd, ol, ul, li,
form, label,
table, caption, tbody, tfoot, thead, tr, th, td { margin: 0; padding: 0;}
img { border: none; vertical-align: top;}
a { text-decoration: none; outline: none;-webkit-tap-highlight-color:rgba(0,0,0,0); display: block;}
ul { list-style: none; }
table { border-collapse: collapse; border-spacing: 0; }
input,button,textarea,select,optgroup,option{ font-size: 100%; font: inherit; background: none; border: none; outline: none;}
h1,h2,h3,h4,h5,h6 { font-weight: normal;}
html {
	font-size: 62.5%;
}
body {
	font-family: "helvetica neue", helvetica, STHeiTi, roboto, "droid sans", arial, \5FAE\8F6F\96C5\9ED1;
	background-color: #eee;
}
* {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.clearfix:after {
	content: "\20";
	display: block;
	clear: both;
	height: 0;
}
i{
	display: block;
}
img{
	width: 100%;
	border: 0
}
.container {
	max-width: 540px;
	margin:4.4rem auto 0;
	z-index: 9
}
section ul li{
	padding: 0 1rem;
}
input[type="button"]{
	background: #00bc8d;
	width: 100%;
	font-size: 1.4rem;
	color: #fff;
	line-height: 3.6rem;
	height: 3.6rem;
	border-radius: .3rem;
}
/*图标*/
.ico {
	background: 0 0 no-repeat;
	background-size: 100% 100%;
	display: inline-block;
}
/*header*/
.header {
	width: 100%;
	height: 4.4rem;
	background-color: #00bc8d;
	position: fixed;
	top: 0;
	z-index: 10;
}
.header h1 {
	font-size: 1.6rem;
	color: #fff;
	line-height: 4.4rem;
	text-align: center;
	font-weight: normal;
}
.header .ico {
	position: absolute;
	bottom: 0;
	display: block;
}
.header-back {
	width: 3.5rem;
	height: 4.4rem;
	left: 1rem;
	background-image:url(../img/ico_goback.png)
}

/*留白*/
.m-top{
	margin-top:.6rem
}
/*背景*/
.bg-fff{
	background-color:#fff;
}

/*弹出层*/
.popbox{
	width:90%;
	height:18.6rem;
	overflow:hidden;
	overflow-y:auto;
	background-color:#fff;
	position:absolute;
	top:0;
	left:0;
	display:none;
	z-index:120;
	border-radius:.5rem;
	-moz-border-radius:.5rem;
	-o-border-radius:.5rem;
	-webkit-border-radius:.5rem;
}
.popbox h2{
	font-size:1.1rem;
	color:#333;
	text-align:center;
	line-height:3rem;
	border-bottom:1px solid #c8c8c8;
	margin-bottom:1rem
}
.popbox p{
	font-size:1rem;
	color:#999;
	padding:0 1rem;
	line-height:1.8rem
}
.popbox a{
	font-size:1.1rem;
	color:#999;
	text-align:center;
	line-height:3rem;
	border-top:1px solid #c8c8c8;
	margin-top:1rem
}
#screen{
	width:100%;
	height:100%;
	position:absolute;
	top:0;left:0;
	display:none;
	z-index:100;
	background-color:#666;
	opacity:0.5;
	filter:alpha(opacity=50);
	-moz-opacity:0.5;
}
.max-height{
	max-height:400px;
	overflow:hidden;
	overflow-y:auto
}
/*字体颜色*/
.c-ff1e3c{
	color:#ff1e3c
}
.c-00bc8d{
	color:#00bc8d
}
select1 {
  appearance:none;
  -moz-appearance:none;
  -webkit-appearance:none;
  background:url(../img/ico_jt_more.png) no-repeat scroll right center transparent;
  padding-right:1.6rem;
  background-size:1.4rem 3.1rem
}
select::-ms-expand { display: none; }
/*浮动*/
.left{
	float:left
}
/*checkbox切换开关*/
.slideOne {
  width: 3.7rem;
  height: 2.2rem;
  background: #ddd;
  margin: .45rem auto;
  position: relative;
  -moz-border-radius: 50px;
  -webkit-border-radius: 50px;
  border-radius: 50px;
  -moz-box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.5), 0px 1px 0px rgba(255, 255, 255, 0.2);
  -webkit-box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.5), 0px 1px 0px rgba(255, 255, 255, 0.2);
  box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.5), 0px 1px 0px rgba(255, 255, 255, 0.2);
}
.slideOne label {
  display: block;
  width: 2rem;
  height: 2rem;
  position: absolute;
  top: .1rem;
  left: .1rem;
  cursor: pointer;
  background: #fff;
  -moz-border-radius: 50px;
  -webkit-border-radius: 50px;
  border-radius: 50px;
  -moz-box-shadow: 0px 2px 5px 0px rgba(0, 0, 0, 0.3);
  -webkit-box-shadow: 0px 2px 5px 0px rgba(0, 0, 0, 0.3);
  box-shadow: 0px 2px 5px 0px rgba(0, 0, 0, 0.3);
  -moz-transition: all 0.4s ease;
  -o-transition: all 0.4s ease;
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
}
.slideOne input[type=checkbox] {
  visibility: hidden;
}
.slideOne input[type=checkbox]:checked + label {
  left:1.6rem;
}

