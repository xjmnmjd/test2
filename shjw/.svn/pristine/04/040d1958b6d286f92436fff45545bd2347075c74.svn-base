}
/*底部*/
.bootom-txt{
	position:fixed;
	bottom:0;
	width:100%;
	text-align:center;
	font-size:13px;
	color:#fff;
	letter-spacing:1px;
	line-height:50px;
	background-color:#666
}
/*right*/
.right{
	position:absolute;
	left:160px;
	padding:20px;
	background-color:#fff;
	
}
.right .up ul li{
	border:1px solid #ddd;
	border-radius:4px;
	-moz-border-radius:4px;
	-o-border-radius:4px;
	-webkit-border-radius:4px;
