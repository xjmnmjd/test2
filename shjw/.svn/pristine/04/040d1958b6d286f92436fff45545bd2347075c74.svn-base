@charset "utf-8";
/* CSS Document */

body,html{ height:100%;}
html, body, div, span,
h1, h2, h3, h4, h5, h6, p, em, img, strong, i,
dl, dt, dd, ol, ul, li,
form, label,
table, caption, tbody, tfoot, thead, tr, th, td { margin: 0; padding: 0;}
img { border: none; vertical-align: top;}
a { text-decoration: none; outline: none;-webkit-tap-highlight-color:rgba(0,0,0,0); display: block;}
ul { list-style: none; }
a:hover{ text-decoration:none}
table { border-collapse: collapse; border-spacing: 0; }
input,button,textarea,select,optgroup,option{ font-size: 100%; font: inherit; background: none; border: none; outline: none;}
h1,h2,h3,h4,h5,h6 { font-weight: normal;}
html {
	font-size: 62.5%;
}
body {
	font-family: "helvetica neue", helvetica, STHeiTi, roboto, "droid sans", arial, \5FAE\8F6F\96C5\9ED1;
}
* {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.clearfix:after {
	content: "\20";
	display: block;
	clear: both;
	height: 0;
}
i{
	display: block;
}
img{
	width: 100%;
	border: 0
}
select{
	appearance:none; 
	-moz-appearance:none;
	-webkit-appearance:none;  
	background:url(../img/ico_select.png) right 0  no-repeat; 
	background-size:18px 30px;
	font-size:14px; 
	font-family:Microsoft YaHei;
} 
/*登录*/
.sex{
	float:left;
	height:38px;
    margin-left:15px
}
.login{
	position:relative;
	overflow:hidden
}
.login-left{
	position:absolute;
	top:0
}
.login-right{
	position:absolute;
	top:0
}
.login-center{
	position:absolute;
	top:0;
	width:1280px;
	background:url(../img/center_bg.jpg) no-repeat;
}
.main{
	width:420px;
	margin:auto
}
.main img{
	width:352px;
	margin:0 34px 55px 34px
}
.login-input{
	background:rgba(0,0,0,.3);
	width:420px;
	height:400px
}
.input{
	width:320px;
	padding-left:50px;
	line-height:50px;
	border:1px solid #fff;
	border-radius:5px;
	-moz-border-radius:5px;
	-o-border-radius:5px;
	-webkit-border-radius:5px;
	color:#fff;
	font-size:18px;
	float:left;
	height:50px
}
.name{
	background:url(../img/ico_name.png) 10px center no-repeat;
	background-size:20px 48px;
}
.password{
	background:url(../img/ico_password.png) 10px center no-repeat;
	background-size:20px 48px;
}
.code{
	background:url(../img/ico_code.png) 10px center no-repeat;
	background-size:20px 48px;
	width:210px;
}
.login-input div{
	width:346px;
	overflow:hidden;
	margin:15px 0 15px 50px;
}
.login-input div:nth-child(1){
	padding-top:55px;
}
.login-input div img{
	width:97px;
	height:40px;
	float:right;
	margin:5px 0
}
.login-input div.login-btn{
	width:320px;
	background-color:#00bc8d;
	line-height:50px;
	height:50px;
	font-size:18px;
	color:#fff;
	border-radius:5px;
	-moz-border-radius:5px;
	-o-border-radius:5px;
	-webkit-border-radius:5px;
	text-align:center;
	margin-top:30px
}
.login-btn input{
	color:#fff
}
.ico-right{
	background:url(../img/ico_right.png) no-repeat;
	background-size:18px 18px;
	width:18px;
	height:18px;
	float:right;
	margin-top:14px
}
.ico-error{
	background:url(../img/ico_error.png) no-repeat;
	background-size:18px 18px;
	width:18px;
	height:18px;
	float:right;
	margin-top:14px
}
.bootom{
	position:absolute;
	font-size:13px;
	color:#fff;
	bottom:20px;
	letter-spacing:1px
}
/*header*/
