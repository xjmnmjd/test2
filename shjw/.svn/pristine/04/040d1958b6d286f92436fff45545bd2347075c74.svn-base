#screen{
	width:100%;
	height:100%;
	position:absolute;
	top:0;left:0;
	display:none;
	z-index:100;
	background-color:#666;
	opacity:0.5;
	filter:alpha(opacity=50);
	-moz-opacity:0.5;
}
.btn{
	border-top:1px solid #ddd;
	height:64px;
	width:100%;
	overflow:hidden
}
.btn div{
	width:164px;
	margin:auto
}
.btn a{
	width:68px;
	height:36px;
	line-height:36px;
	font-size:14px;
	border-radius:4px;
	-moz-border-radius:4px;
	-o-border-radius:4px;
	-webkit-border-radius:4px;
	background-color:#00bc8d;
	color:#fff;
	float:left;
	margin-top:10px;
	text-align:center
}
.btn input{
	width:68px;
	height:36px;
	line-height:36px;
	font-size:14px;
	border-radius:4px;
	-moz-border-radius:4px;
	-o-border-radius:4px;
	-webkit-border-radius:4px;
	background-color:#00bc8d;
	color:#fff;
